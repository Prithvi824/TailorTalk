# OpenAI or other LLM key
OPENAI_API_KEY=

# Google service account credentials JSON file path (absolute or relative)
GOOGLE_CREDS_JSON=service-account.json
# Calendar ID (email) to book on
CALENDAR_ID=

# FastAPI backend URL for Streamlit when deployed (override if needed)
API_URL=https://<your-render-backend-url>/chat

# Default timezone
TIMEZONE=Asia/Kolkata
